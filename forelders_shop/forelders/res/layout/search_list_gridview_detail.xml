<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:background="@drawable/bg_etbox_blue" >
	<ImageView
		android:id="@+id/search_list_img"
		android:layout_width="@dimen/kaka_20_dip"
		android:layout_height="@dimen/kaka_20_dip"
		android:layout_centerVertical="true"
		android:layout_marginRight="@dimen/kaka_2_dip"
		android:layout_toLeftOf="@+id/search_list_title_text"
		android:contentDescription="@null"
		android:scaleType="fitXY"
		android:src="@drawable/img_default"
		android:visibility="gone" />
	<TextView
		android:id="@+id/search_list_title_text"
		style="@style/black_h6_text"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_centerInParent="true"
		android:contentDescription="@null"
		android:ems="5"
		android:gravity="center"
		android:paddingBottom="@dimen/kaka_5_dip"
		android:paddingTop="@dimen/kaka_5_dip"
		android:singleLine="true"
		android:text="@string/soft_name" />
</RelativeLayout>